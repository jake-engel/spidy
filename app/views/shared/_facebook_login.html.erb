<%- if devise_mapping.omniauthable? %>
<%- resource_class.omniauth_providers.each do |provider| %>
<%= link_to omniauth_authorize_path(resource_name, provider), class: "btn btn-facebook btn-lg startBtn" do %>
<span class="fa fa-<%= provider %>">Continue with <%= OmniAuth::Utils.camelize(provider) %></span>
<% end %><br />
<% end -%>
<% end -%>
